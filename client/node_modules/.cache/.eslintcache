   </div>\r\n        );\r\n    }\r\n    else {\r\n        return (\r\n            <div className='gameChoices'>\r\n                <GameChoice name=\"A\" text={choices[0]} id={\"A\" === props.correct ? \"A\" : \"O\"}></GameChoice>\r\n                <GameChoice name=\"B\" text={choices[1]} id={\"B\" === props.correct ? \"B\" : \"O\"}></GameChoice>\r\n                <GameChoice name=\"C\" text={choices[2]} id={\"C\" === props.correct ? \"C\" : \"O\"}></GameChoice>\r\n                <GameChoice name=\"D\" text={choices[3]} id={\"D\" === props.correct ? \"D\" : \"O\"}></GameChoice>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nfunction InGame(props) {\r\n    const data = props.data;\r\n    console.log(data);\r\n    const [correct, setCorrect] = useState('');\r\n    return (\r\n        <div>\r\n            <HostGameHeader text={data.q}></HostGameHeader>\r\n            <GameChoices correct={correct} choices={data.a}></GameChoices>\r\n        </div >\r\n    )\r\n}\r\n\r\nfunction rankify(place) {\r\n    let dig = parseInt(place, 10) % 10;\r\n    let end;\r\n    switch (dig) {\r\n        case '':\r\n            end = '';\r\n            break;\r\n        case 1:\r\n            end = \"st\";\r\n            break;\r\n        case 2:\r\n            end = \"nd\";\r\n            break;\r\n        case 3:\r\n            end = \"rd\";\r\n            break;\r\n        default:\r\n            end = \"th\";\r\n            break;\r\n    }\r\n    return place + end;\r\n}\r\n\r\nfunction PlayerRank(props) {\r\n    return (\r\n        <div className='playerRank'>\r\n            <span className='rank'>{props.rank}</span>\r\n            <span className='name'>{props.name}</span>\r\n            <span className='score'>{props.score}</span>\r\n        </div>\r\n    );\r\n}\r\n\r\nfunction PlayerBox(props) {\r\n    if (props.id === \"startBtn\") {\r\n        return (\r\n            <div onClick={props.click} id={props.id} className='playerBox'>\r\n                <p>{props.name}</p>\r\n            </div>\r\n        );\r\n    }\r\n    else {\r\n        return (\r\n            <div className='playerBox'>\r\n                <p>{props.name}</p>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nfunction Scores(props) {\r\n    const data = props.data;\r\n    let places = data.map((user, index) => {\r\n        return <PlayerRank key={index} rank={rankify(index + 1)} name={user.name} score={user.score}></PlayerRank>\r\n    });\r\n\r\n    return (\r\n        <div>\r\n            <HostGameHeader text={'Scores'}></HostGameHeader>\r\n            <div className='playerRanks'>\r\n                {places}\r\n            </div>\r\n        </div>\r\n    );\r\n}\r\n\r\nfunction Wait(props) {\r\n    const data = props.data;\r\n    console.log(\"Waiting...\")\r\n    // console.log(data);\r\n    let users;\r\n    if (data) {\r\n        users = data.map((user, index) => {\r\n            return <PlayerBox key={index} name={user.name}></PlayerBox>\r\n        });\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <HostGameHeader text={`Join with code: ${props.code}`}></HostGameHeader>\r\n            {/* <div onClick={props.click}>Start</div> */}\r\n            <div className='playerBoxes'>\r\n                <PlayerBox click={props.click} id=\"startBtn\" name=\"Start\"></PlayerBox>\r\n                {users}\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nfunction HostGame() {\r\n    const { socket } = useContext(GlobalContext);\r\n\r\n    const [qs, setQs] = useState();\r\n    const code = '1234';\r\n\r\n\r\n    socket.emit('hostGame', code);\r\n\r\n    const [data, setData] = useState();\r\n    //     { name: \"Nick\", score: 120 },\r\n    //     { name: \"Jason\", score: 110 },\r\n    //     { name: \"Paul\", score: 50 },\r\n    //     { name: \"Nash\", score: 20 },\r\n    //     { name: \"Max\", score: 10 },\r\n    //     { name: \"Sasha\", score: 8 },\r\n    //     { name: \"Michael\", score: 8 },\r\n    //     { name: \"Kyle\", score: 6 },\r\n    //     { name: \"Pranay\", score: 1 }\r\n    // ]);\r\n    // console.log(data)\r\n    const [mode, setMode] = useState('wait');\r\n    const startGame = () => {\r\n        socket.emit('startGame', { code: code });\r\n        // setMode('play');\r\n    };\r\n    useState(() => {\r\n        socket.on('startGame', (question) => {\r\n            setQs({ q: question.q, a: question.a });\r\n            setMode('play');\r\n        });\r\n    }, [socket, setMode]);\r\n    useState(() => {\r\n        socket.on('playerChange', ({ players }) => {\r\n            console.log(players);\r\n            setData(players);\r\n        });\r\n    }, [socket, setData]);\r\n    if (mode == 'play') {\r\n        return (\r\n            <InGame data={qs}></InGame>\r\n        );\r\n    }\r\n    else if (mode == 'scores') {\r\n        return (\r\n            <Scores data={data}></Scores>\r\n        );\r\n    }\r\n    else if (mode == 'wait') {\r\n        return (\r\n            <Wait data={data} code={code} click={startGame}></Wait>\r\n        );\r\n    }\r\n}\r\n\r\nexport default HostGame;","C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\pages\\StartGame.js",[],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\pages\\UserInfo.js",[],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\items\\Box.js",[],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\items\\index.js",[],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\items\\BackButton.js",[],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\items\\GameButton.js",[],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\items\\GameButtons.js",[],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\items\\GameHeader.js",[],"C:\\Users\\schac\\Desktop\\creativeprojec