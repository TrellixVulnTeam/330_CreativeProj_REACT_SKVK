 {\r\n            setCorr(c);\r\n            setClicked(\"\");\r\n            setPlaying(true);\r\n        });\r\n    }, [socket, setPlaying]);\r\n\r\n    const userClick = ((n) => {\r\n        if (clicked === \"\") {\r\n            setClicked(n);\r\n            console.log(corr + ' ' + n);\r\n            let correct = corr === n;\r\n            socket.emit('userClick', { user: username, ans: correct });\r\n        }\r\n    });\r\n\r\n    return (\r\n        <div>\r\n            {playing && (<InGame username={username} rank={rank} score={score} clicked={clicked} click={n => userClick(n)}></InGame>)}\r\n            {!playing && (<Waiting username={username} rank={rank} score={score} message={\"The game will begin soon!\"}></Waiting>)}\r\n        </div>\r\n    )\r\n}\r\nexport default Game;","C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\pages\\SignUp.js",[],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\pages\\SavedGames.js",[],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\pages\\CreateGame.js",[],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\pages\\StartScreen.js",[],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\pages\\HostGame.js",["170"],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\pages\\UserInfo.js",[],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\items\\Box.js",[],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\items\\index.js",[],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\items\\BackButton.js",[],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\items\\GameButton.js",[],"C:\\Users\\schac\\Desktop\\creativeproject-477477-475454\\client\\src\\components\\items\\GameButtons.js",[],"C:\\Users\\s